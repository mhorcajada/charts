{
  "regexManagers": [
    {
      "fileMatch": ["^storj-gateway/values.yaml$"],
      "matchStrings": [
        "tag:\\s*\"?(?<currentValue>[a-z0-9]+-v(?<version>[0-9]+\\.[0-9]+\\.[0-9]+))\"?"
      ],
      "depNameTemplate": "storjlabs/gateway",
      "datasourceTemplate": "docker",
      "versioningTemplate": "semver"
    }
  ]
}